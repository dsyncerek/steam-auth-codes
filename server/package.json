{
  "name": "steam-auth-codes-server",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "nest start --watch --path tsconfig.json",
    "start:prod": "node dist/main.js",
    "prebuild": "rimraf dist",
    "build": "nest build --path tsconfig.json",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src/**/*.ts --fix",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nestjs/platform-socket.io": "^6.8.3",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/websockets": "^6.8.3",
    "dotenv": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "steam-totp": "^2.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.10.5",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard-with-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "~3.6.4"
  }

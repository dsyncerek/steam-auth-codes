  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.4.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
